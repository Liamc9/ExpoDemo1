{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": { "simulator": false },
      "android": {}
    },
    "preview": {
      "distribution": "internal",
      "channel": "staging",
      "ios": {},
      "android": {}
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "production",
      "ios": {},
      "android": {}
    }
  }
}
